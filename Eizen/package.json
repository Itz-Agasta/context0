{
	"name": "eizendb",
	"version": "1.0.0",
	"description": "Vector database backend lib for ArchiveNet",
	"main": "lib/index.cjs",
	"module": "lib/index.js",
	"types": "lib/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"import": {
				"types": "./lib/index.d.ts",
				"default": "./lib/index.js"
			},
			"require": {
				"types": "./lib/index.d.cts",
				"default": "./lib/index.cjs"
			}
		}
	},
	"files": ["lib", "proto", "README.md", "package.json"],
	"scripts": {
		"build": "tsdown",
		"dev": "tsx src/index.ts",
		"check": "biome check --write .",
		"check:ci": "biome check .",
		"proto:clean": "rm -rf proto/*.js proto/*.d.ts",
		"proto:gen": "npm run proto:clean && npx pbjs ./proto/hnsw_comm.proto -w commonjs -t static-module -o ./proto/hnsw_comm.js && npx pbts ./proto/hnsw_comm.js -o ./proto/hnsw_comm.d.ts && npx pbjs ./proto/request.proto -w commonjs -t static-module -o ./proto/request.js && npx pbts ./proto/request.js -o ./proto/request.d.ts"
	},
	"keywords": ["vector-database",
		"hnsw",
		"arweave",
		"blockchain",
		"similarity-search",
		"machine-learning",
		"nearest-neighbor",
		"embeddings",
		"nodejs",
		"typescript"],
		"author": "Agasta",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/Itz-Agasta/ArchiveNET/tree/main/Eizen"
	},
	"homepage": "https://github.com/Itz-Agasta/ArchiveNET/tree/main/Eizen#readme",
	"bugs": {
		"url": "https://github.com/Itz-Agasta/ArchiveNET/issues"
	},
	"devDependencies": {
		"@biomejs/biome": "2.0.0",
		"@tsconfig/node23": "23.0.2",
		"@types/node": "24.0.3",
		"protobufjs-cli": "^1.1.3",
		"tsdown": "^0.12.8",
		"tsx": "4.20.3",
		"typescript": "5.8.3"
	},
	"dependencies": {
		"heap-js": "^2.6.0",
		"hollowdb": "^1.4.3",
		"protobufjs": "^7.5.3",
		"warp-contracts": "^1.4.45",
		"warp-contracts-plugin-deploy": "^1.0.13"
	}
}
